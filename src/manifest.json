{
    "manifest_version": 2,

    "minimum_chrome_version": "20",

    "name": "Iframely for Gmail",
    "description": "As seen on Lifehacker.com: Watch embedded videos, photos and other media - right in your Gmail",
    "version": "0.1.7",
    "icons": {
        "128": "icons/iframely-icon-128x128.png"
    },
    "content_scripts": [ {
        "js": [
            "js/vendor/jquery-1.9.1.min.js",
            "js/vendor/iframely.js",
            "js/links_extractor.js"
        ],
        "matches": [ "https://mail.google.com/*" ],
        "run_at": "document_end"
    } ],
    "background" : {
        "scripts": ["js/background.js"]
    },
    "options_page": "options/options.html",
    "web_accessible_resources": ["css/styles.css", "icons/icons.svg"]
}